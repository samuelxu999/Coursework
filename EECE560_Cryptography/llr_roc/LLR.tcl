#!/usr/bin/tclsh

array set LLR { A 0.6572604212264213 B -1.2070697408364692 C -0.06017945190256874 D 0.0261486315859286 E 0.9610618755580854 F -0.21262393783357816 G -0.3783770682803 H -0.4274489622726971 I 0.761620436550664 J -4.3045847087004026 K -2.243161672523245 L 0.39719351580761364 M -0.3369918521174457 N 0.4685182966295439 O 0.6411310392965376 P 0.14643479746043048 Q -3.0724410274077703 R 0.5727912645999712 S 0.5339622048819276 T 0.8345694361968317 U -0.15918497567801773 V -1.4798102332900511 W -0.7656979242650609 X -1.430213292150679 Y -1.2070697408364692 Z -4.3045847087004026 }

set data [string toupper [read stdin]]
set sum 0.0

foreach x [split ABCDEFGHIJKLMNOPQRSTUVWXYZ ""] {
	set count [regexp -all $x $data]
	set sum [expr $sum+$count*$LLR($x)]
}

puts $sum
